# minimization_2n/Makefile.bench				* -*- Makefile -*-

bench: all
	@mkdir bool;					\
	mkdir z;					\
	echo '================';			\
	echo '|Quotient bench|';			\
	echo '================';			\
	for n in $$(seq 5 13); do			\
	    echo "----------------------For power $$n";	\
	    echo "--------------VCSN (over Z):";	\
	    ./quotient_z_bench $$n;			\
	    echo "--------------VCSN (over B):";	\
	    ./quotient_bool_bench $$n;			\
	done;

bench-clean:
	rm -rf bool
	rm -rf z
