
<transducer xmlns="http://vaucanson.lrde.epita.fr">

  <type>
    <monoid generators="letters" type="product">
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </type>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s1" in="a" out="1" src="s0"/>
      <transition dst="s0" in="1" out="b" src="s1"/>
      <transition dst="s2" in="b" out="1" src="s1"/>
      <transition dst="s1" in="1" out="a" src="s2"/>
      <initial state="s1"/>
      <final state="s0"/>
      <final state="s1"/>
    </transitions>
  </content>

</transducer>
