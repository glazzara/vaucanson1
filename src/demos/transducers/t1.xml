<transducer name="t1" xmlns="http://vaucanson.lrde.epita.fr">
  <type>
    <monoid generators="letters" type="product">
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </type>
  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition src="s0" dst="s1" in="1" out="b"/>
      <transition src="s1" dst="s0" in="a" out="1"/>
      <transition src="s1" dst="s2" in="1" out="a"/>
      <transition src="s2" dst="s1" in="b" out="1"/>
      <initial state="s1"/>
      <final state="s0"/>
      <final state="s1"/>
    </transitions>
  </content>
</transducer>
