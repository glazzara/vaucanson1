
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <type>
    <monoid generators="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring operations="numerical" set="Z"/>
  </type>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
    </states>
    <transitions>
      <transition dst="s0" label="a" src="s0"/>
      <transition dst="s0" label="b" src="s0"/>
      <transition dst="s1" label="b" src="s0"/>
      <transition dst="s1" label="(2 a)" src="s1"/>
      <transition dst="s1" label="(2 b)" src="s1"/>
    </transitions>
    <initials>
      <initial label="1" state="s0"/>
    </initials>
    <finals>
      <final label="1" state="s1"/>
    </finals>
  </content>

</automaton>
