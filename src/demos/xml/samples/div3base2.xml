
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <type>
    <monoid generators="letters" type="free">
      <generator value="0"/>
      <generator value="1"/>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </type>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s0" label="0" src="s0"/>
      <transition dst="s1" label="1" src="s0"/>
      <transition dst="s2" label="0" src="s1"/>
      <transition dst="s0" label="1" src="s1"/>
      <transition dst="s1" label="0" src="s2"/>
      <transition dst="s2" label="1" src="s2"/>
    </transitions>
    <initials>
      <initial state="s0"/>
    </initials>
    <finals>
      <final state="s0"/>
    </finals>
  </content>

</automaton>
