
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <type>
    <monoid generators="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </type>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s1" label="a" src="s0"/>
      <transition dst="s2" label="b" src="s0"/>
      <transition dst="s1" label="a" src="s1"/>
      <transition dst="s1" label="b" src="s1"/>
      <transition dst="s2" label="a" src="s2"/>
      <transition dst="s2" label="b" src="s2"/>
      <initial state="s0"/>
      <final state="s1"/>
    </transitions>
  </content>

</automaton>
