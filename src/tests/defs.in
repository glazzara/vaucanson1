# User can set VERBOSE to see all output.
test -z "$VERBOSE" && {
   exec > /dev/null 2>&1
}

CXX=@CXX@
abs_top_builddir=@abs_top_builddir@
abs_top_srcdir=@abs_top_builddir@

AM_CXXFLAGS="-Wno-unused -Wall -W -ftemplate-depth-50 -pedantic -I$abs_top_builddir -I$abs_top_srcdir -I$abs_top_srcdir/include"

# Turn on shell traces when VERBOSE=x.
if test "x$VERBOSE" = xx; then
  set -x
else
  :
fi
