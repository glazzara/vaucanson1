<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

  <automaton>
    <valueType>
      <semiring operations="classical" set="Z" type="numerical"/>
      <monoid prodDim="2" type="product">
        <monoid genDescrip="enum" genKind="simple" genSort="integers" type="free">
          <monGen value="1"/>
          <monGen value="10"/>
        </monoid>
        <monoid genDescrip="enum" genKind="simple" genSort="integers" type="free">
          <monGen value="2"/>
          <monGen value="20"/>
        </monoid>
      </monoid>
    </valueType>
    <automatonStruct>
      <states>
        <state id="s0"/>
        <state id="s1"/>
        <state id="s2"/>
      </states>
      <transitions>
        <transition src="s0" target="s1">
          <label>
            <leftExtMul>
              <weight value="10"/>
              <monElmt>
                <monElmt>
                  <monGen value="1"/>
                </monElmt>
                <monElmt>
                  <monGen value="20"/>
                </monElmt>
              </monElmt>
            </leftExtMul>
          </label>
        </transition>
        <transition src="s0" target="s2">
          <label>
            <zero/>
          </label>
        </transition>
        <transition src="s0" target="s1">
          <label>
            <leftExtMul>
              <weight value="10"/>
              <monElmt>
                <monElmt>
                  <monGen value="1"/>
                </monElmt>
                <monElmt>
                  <monGen value="2"/>
                </monElmt>
              </monElmt>
            </leftExtMul>
          </label>
        </transition>
        <initial state="s0"/>
        <final state="s2">
          <label>
            <leftExtMul>
              <weight value="10"/>
              <monElmt>
                <monElmt>
                  <monGen value="10"/>
                  <monGen value="10"/>
                </monElmt>
                <monElmt>
                  <monGen value="2"/>
                </monElmt>
              </monElmt>
            </leftExtMul>
          </label>
        </final>
      </transitions>
    </automatonStruct>
  </automaton>

</fsmxml>
