<?xml version="1.0" encoding="ISO8859-1" standalone="no" ?>
<!DOCTYPE automaton SYSTEM "vaucanson.dtd">
<automaton xmlns="http://www.lrde.epita.fr/vaucanson">
 <type>
  <monoid>
    <generator value="A"/>
    <generator value="B"/>
    <generator value="C"/>
  </monoid>

  <semiring set="R" operations="numerical"/>
 </type>

 <content>
  <states>
    <state name="zero"/>
    <state name="one"/>
    <state name="two"/>
    <state name="three"/>
  </states>

  <transitions>
    <transition label="(2 C)" src="zero" dst="one"/>
    <transition label="(3 B)" src="two" dst="one"/>
    <transition label="(1 A)" src="one" dst="one"/>
    <transition label="(8 1)" src="two" dst="three"/>
    <transition label="(2 C)" src="three" dst="three"/>
  </transitions>

  <initials>
    <initial state="zero"/>
  </initials>

  <finals>
    <final state="two"/>
  </finals>
 </content>
</automaton>
