<?xml version="1.0" encoding="ISO8859-1" standalone="no" ?>
<!DOCTYPE automaton SYSTEM "vaucanson.dtd">
<automaton xmlns="http://www.lrde.epita.fr/vaucanson">

  <type>
    <monoid generators="letters" type="free">
      <generator value="A"/>
      <generator value="B"/>
      <generator value="C"/>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </type>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
      <state name="s3"/>
    </states>
    <transitions>
      <transition dst="s1" label="C" src="s0"/>
      <transition dst="s1" label="B" src="s2"/>
      <transition dst="s1" label="A" src="s1"/>
      <transition dst="s3" label="A" src="s2"/>
      <transition dst="s3" label="C" src="s3"/>
    </transitions>
    <initials>
      <initial label="1" state="s0"/>
    </initials>
    <finals>
      <final label="1" state="s2"/>
    </finals>
  </content>

</automaton>

