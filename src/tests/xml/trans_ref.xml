
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <numericalSemiring>
      <monoid generatorType="letters" type="free">
        <generator value="x"/>
        <generator value="y"/>
      </monoid>
      <semiring operations="classical" set="B"/>
    </numericalSemiring>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s0" src="s0"/>
      <transition dst="s1" in="a" out="y" src="s0"/>
      <transition dst="s2" in="b" out="x" src="s1"/>
      <initial state="s0"/>
      <final out="x" state="s1"/>
    </transitions>
  </content>

</automaton>
