<transducer xmlns="http://vaucanson.lrde.epita.fr">
  <labelType>
    <monoid generators="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring set="ratSeries">
      <monoid generators="letters" type="free">
        <generator value="x"/>
        <generator value="y"/>
      </monoid>
      <semiring operations="numerical" set="B"/>
    </semiring>
  </labelType>
  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition src="s0" dst="s0"/>
      <transition src="s0" dst="s1" in="a" out="y"/>
      <transition src="s1" dst="s2" in="b" out="x"/>
      <initial state="s0"/>
      <final out="x" state="s1"/>
    </transitions>
  </content>
</transducer>
