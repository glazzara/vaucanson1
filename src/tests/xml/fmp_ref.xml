<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="0.1">

<automaton>
  <valueType>
    <semiring operations="classical" set="B" type="numerical"/>
    <monoid type="product" prodDim="2">
      <monoid genSort="letters" type="free" genDescrip="enum" genKind="simple">
        <monGen value="a"/>
        <monGen value="b"/>
      </monoid>
      <monoid genSort="letters" type="free" genDescrip="enum" genKind="simple">
        <monGen value="x"/>
        <monGen value="y"/>
      </monoid>
    </monoid>
  </valueType>
  <automatonStruct>
    <states>
      <state id="s0"/>
      <state id="s1"/>
      <state id="s2"/>
    </states>
    <transitions>
      <transition src="s0" target="s1">
	<label>
	  <monElmt>
	    <monElmt>
	      <monGen value="a"/>
	    </monElmt>
	    <monElmt>
	      <monGen value="x"/>
	    </monElmt>
	  </monElmt>
	</label>
      </transition>
      <transition src="s0" target="s2">
	<label>
	  <zero/>
	</label>
      </transition>
      <transition src="s0" target="s1">
	<label>
	  <monElmt>
	    <monElmt>
	      <monGen value="a"/>
	    </monElmt>
	    <monElmt>
	      <monGen value="y"/>
	    </monElmt>
	  </monElmt>
	</label>
      </transition>
      <initial state="s0"/>
      <final state="s2">
	<label>
	  <monElmt>
	    <monElmt>
	      <monGen value="b"/>
	    </monElmt>
	    <monElmt>
	      <monGen value="x"/>
	    </monElmt>
	  </monElmt>
	</label>
      </final>
    </transitions>
  </automatonStruct>
</automaton>

</fsmxml>
