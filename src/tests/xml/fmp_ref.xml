<transducer xmlns="http://vaucanson.lrde.epita.fr">
  <label_type>
    <monoid generators="letters" type="product">
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generators="letters" type="free">
        <generator value="x"/>
        <generator value="y"/>
      </monoid>
    </monoid>
    <semiring operations="numerical" set="B"/>
  </label_type>
  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition src="s0" dst="s1" in="a" out="x"/>
      <transition src="s0" dst="s1" in="a" out="y"/>
      <initial state="s0"/>
      <final in="b" out="x" state="s2"/>
    </transitions>
  </content>
</transducer>
