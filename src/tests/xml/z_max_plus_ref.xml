
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring operations="maxPlus" set="Z"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s1" label="a" src="s0"/>
      <transition dst="s2" label="b" src="s1"/>
      <initial label="1" state="s0"/>
      <final label="1" state="s1"/>
    </transitions>
  </content>

</automaton>
