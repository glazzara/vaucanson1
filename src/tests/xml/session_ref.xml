<session xmlns="http://vaucanson.lrde.epita.fr">
  <automaton>
    <label_type>
      <monoid generators="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <semiring operations="numerical" set="B"/>
    </label_type>
    <content>
      <states>
        <state name="s0"/>
        <state name="s1"/>
        <state name="s2"/>
      </states>
      <transitions>
        <transition src="s0" dst="s1" label="a"/>
        <transition src="s1" dst="s2" label="b"/>
      </transitions>
    </content>
  </automaton>
