<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="0.1">

<automaton>
  <valueType>
    <semiring type="series">
      <semiring operations="classical" set="Z" type="numerical"/>
      <monoid genSort="letters" type="free" genDescrip="enum" genKind="simple">
        <monGen value="x"/>
        <monGen value="y"/>
      </monoid>
    </semiring>
    <monoid genSort="letters" type="free" genDescrip="enum" genKind="simple">
      <monGen value="a"/>
      <monGen value="b"/>
    </monoid>
  </valueType>
  <automatonStruct>
    <states>
      <state id="s0"/>
      <state id="s1"/>
      <state id="s2"/>
    </states>
    <transitions>
      <transition src="s0" target="s0">
	<label>
	  <one/>
	</label>
      </transition>
      <transition src="s0" target="s1">
	<label>
	  <leftExtMul>
	    <weight>
	      <monElmt>
		<monGen value="y"/>
	      </monElmt>
	    </weight>
	    <monElmt>
	      <monGen value="a"/>
	    </monElmt>
	  </leftExtMul>
	</label>
      </transition>
      <transition src="s1" target="s2">
	<label>
	  <leftExtMul>
	    <weight>
	      <monElmt>
		<monGen value="x"/>
	      </monElmt>
	    </weight>
	    <monElmt>
	      <monGen value="b"/>
	    </monElmt>
	  </leftExtMul>
	</label>
      </transition>
      <initial state="s0"/>
      <final state="s1"/>
	<label>
	  <leftExtMul>
	    <weight>
	      <monElmt>
		<monGen value="x"/>
	      </monElmt>
	    </weight>
	    <one/>
	  </leftExtMul>
	</label>
      </final>
    </transitions>
  </automatonStruct>
</automaton>

<fsmxml/>
