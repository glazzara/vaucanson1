<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="0.1">

<automaton>
  <valueType>
    <semiring operations="minPlus" set="Z" type="numerical"/>
    <monoid genSort="letters" type="free" genDescrip="enum" genKind="simple">
      <monGen value="a"/>
      <monGen value="b"/>
    </monoid>
  </valueType>
  <automatonStruct>
    <states>
      <state id="s0"/>
      <state id="s1"/>
      <state id="s2"/>
    </states>
    <transitions>
      <transition src="s0" target="s1">
	<label>
	  <monElmt>
	    <monGen value="a"/>
	  </monElmt>
	</label>
      </transition>
      <transition src="s1" target="s2">
	<label>
	  <monElmt>
	    <monGen value="b"/>
	  </monElmt>
	</label>
      </transition>
      <initial state="s0"/>
      <final state="s1"/>
    </transitions>
  </automatonStruct>
</automaton>

</fsmxml>
