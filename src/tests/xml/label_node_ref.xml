
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
      <generator value="c"/>
      <generator value="d"/>
    </monoid>
    <semiring operations="classical" set="Z"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s0" src="s0">
        <label>
          <sum>
            <sum>
              <sum>
                <word value="a" weight="6"/>
                <word value="b" weight="6"/>
              </sum>
              <word value="c" weight="15"/>
            </sum>
            <word value="d" weight="15"/>
          </sum>
        </label>
      </transition>
      <transition dst="s1" src="s0">
        <label>
          <sum>
            <word value="a" weight="6"/>
            <word value="b" weight="5"/>
          </sum>
        </label>
      </transition>
      <transition dst="s2" src="s0">
        <label>
          <sum>
            <word value="aa"/>
            <word value="b" weight="3"/>
          </sum>
        </label>
      </transition>
      <transition dst="s0" src="s1">
        <label>
          <word value="ab" weight="5"/>
        </label>
      </transition>
      <transition dst="s1" src="s1">
        <label>
          <sum>
            <word value="a" weight="5"/>
            <word value="b" weight="5"/>
          </sum>
        </label>
      </transition>
      <initial state="s0">
        <label>
          <identity/>
        </label>
      </initial>
      <final state="s1">
        <label>
          <identity/>
        </label>
      </final>
    </transitions>
  </content>

</automaton>
