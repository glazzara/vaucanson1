
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <labelType>
    <monoid generatorType="letters" type="product">
      <monoid generatorType="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generatorType="letters" type="free">
        <generator value="u"/>
        <generator value="v"/>
      </monoid>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
      <state name="s3"/>
      <state name="s4"/>
      <state name="s5"/>
      <state name="s6"/>
      <state name="s7"/>
    </states>
    <transitions>
      <transition dst="s2" in="a" src="s0"/>
      <transition dst="s3" src="s0"/>
      <transition dst="s4" in="a" src="s1"/>
      <transition dst="s0" out="v" src="s1"/>
      <transition dst="s1" in="b" src="s3"/>
      <transition dst="s5" out="v" src="s3"/>
      <transition dst="s6" src="s4"/>
      <transition dst="s2" out="v" src="s4"/>
      <transition dst="s0" in="b" src="s5"/>
      <transition dst="s7" in="a" src="s6"/>
      <transition dst="s1" out="u" src="s6"/>
      <transition dst="s4" out="u" src="s7"/>
      <initial state="s0"/>
      <initial state="s1"/>
      <final state="s1"/>
      <final state="s4"/>
    </transitions>
  </content>

</automaton>
