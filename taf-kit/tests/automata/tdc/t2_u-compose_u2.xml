
<automaton xmlns="http://vaucanson.lrde.epita.fr">

  <labelType>
    <monoid generatorType="letters" type="product">
      <monoid generatorType="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generatorType="letters" type="free">
        <generator value="u"/>
        <generator value="v"/>
      </monoid>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
      <state name="s4"/>
      <state name="s8"/>
      <state name="s9"/>
      <state name="s10"/>
    </states>
    <transitions>
      <transition dst="s0" out="v" src="s1"/>
      <transition dst="s4" in="a" src="s2"/>
      <transition dst="s10" out="u" src="s8"/>
      <transition dst="s0" out="v" src="s9"/>
      <transition dst="s1" in="b" src="s0"/>
      <transition dst="s2" in="b" src="s0"/>
      <transition dst="s8" in="a" src="s4"/>
      <transition dst="s8" in="a" src="s10"/>
      <transition dst="s9" out="u" src="s4"/>
      <transition dst="s9" out="u" src="s10"/>
      <initial state="s0"/>
      <initial state="s1"/>
      <initial state="s2"/>
      <final state="s1"/>
      <final state="s4"/>
      <final state="s9"/>
      <final state="s10"/>
    </transitions>
  </content>

</automaton>
