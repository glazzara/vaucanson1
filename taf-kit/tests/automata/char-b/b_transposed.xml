<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

<automaton>
  <valueType>
    <semiring operations="classical" set="B" type="numerical"/>
    <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
      <monGen value="a"/>
      <monGen value="b"/>
      <monGen value="c"/>
      <monGen value="d"/>
    </monoid>
  </valueType>
  <automatonStruct>
    <states>
      <state id="s0"/>
      <state id="s1"/>
      <state id="s2"/>
    </states>
    <transitions>
      <transition src="s1" target="s0">
	<label>
	  <monElmt>
	    <monGen value="d"/>
	    <monGen value="c"/>
	    <monGen value="b"/>
	    <monGen value="a"/>
	  </monElmt>
	</label>
      </transition>
      <transition src="s2" dst="s1" label="abcd+ba"/>
      <transition src="s2" dst="s1">
	<label>
	  <sum>
	    <product>
	      <monElmt>
		<monGen value="a"/>
		<monGen value="b"/>
	      </monElmt>
	      <monElmt>
		<monGen value="c"/>
		<monGen value="d"/>
	      </monElmt>
	    </product>
	    <monElmt>
	      <monGen value="b"/>
	      <monGen value="a"/>
	    </monElmt>
	  </sum>
	</label>
      </transition>
      <initial state="s1"/>
      <final state="s0"/>
    </transitions>
  </automatonStruct>
</automaton>

</fsmxml>
