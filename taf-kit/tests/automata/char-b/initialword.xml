
<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

  <automaton>
    <valueType>
      <semiring operations="classical" set="B" type="numerical"/>
      <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
        <monGen value="a"/>
        <monGen value="b"/>
      </monoid>
    </valueType>
    <automatonStruct>
      <states>
        <state id="s0"/>
        <state id="s1"/>
      </states>
      <transitions>
        <transition src="s0" target="s1">
          <label>
            <monElmt>
              <monGen value="a"/>
            </monElmt>
          </label>
        </transition>
        <initial state="s0">
          <label>
            <monElmt>
              <monGen value="a"/>
              <monGen value="b"/>
            </monElmt>
          </label>
        </initial>
        <final state="s1"/>
      </transitions>
    </automatonStruct>
  </automaton>

</fsmxml>
