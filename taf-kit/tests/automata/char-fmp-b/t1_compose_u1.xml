<fsmxml xmlns="http://vaucanson.lrde.epita.fr" version="1.0">

  <automaton>
    <valueType>
      <semiring operations="classical" set="B" type="numerical"/>
      <monoid prodDim="2" type="product">
        <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
          <monGen value="a"/>
          <monGen value="b"/>
        </monoid>
        <monoid genDescrip="enum" genKind="simple" genSort="letters" type="free">
          <monGen value="u"/>
          <monGen value="v"/>
        </monoid>
      </monoid>
    </valueType>
    <automatonStruct>
      <states>
        <state id="s0"/>
        <state id="s1"/>
        <state id="s2"/>
        <state id="s3"/>
        <state id="s4"/>
        <state id="s5"/>
        <state id="s6"/>
        <state id="s7"/>
      </states>
      <transitions>
      <transition dst="s4" out="u" source="s7"/>
        <transition source="s0" target="s2">
          <label>
            <monElmt>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <one/>
            </monElmt>
          </label>
        </transition>
        <transition source="s0" target="s3">
          <label>
	    <one/>
          </label>
        </transition>
        <transition source="s1" target="s4">
          <label>
            <monElmt>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <one/>
            </monElmt>
          </label>
        </transition>
        <transition source="s1" target="s0">
          <label>
            <monElmt>
              <one/>
              <monElmt>
                <monGen value="v"/>
              </monElmt>
            </monElmt>
          </label>
        </transition>
        <transition source="s3" target="s1">
          <label>
            <monElmt>
              <monElmt>
                <monGen value="b"/>
              </monElmt>
              <one/>
            </monElmt>
          </label>
        </transition>
        <transition source="s3" target="s5">
          <label>
            <monElmt>
              <one/>
              <monElmt>
                <monGen value="v"/>
              </monElmt>
            </monElmt>
          </label>
        </transition>
        <transition source="s4" target="s6">
          <label>
	    <one/>
          </label>
        </transition>
        <transition source="s4" target="s2">
          <label>
            <monElmt>
              <one/>
              <monElmt>
                <monGen value="v"/>
              </monElmt>
            </monElmt>
          </label>
        </transition>
        <transition source="s5" target="s0">
          <label>
            <monElmt>
              <monElmt>
                <monGen value="b"/>
              </monElmt>
              <one/>
            </monElmt>
          </label>
        </transition>
        <transition source="s6" target="s7">
          <label>
            <monElmt>
              <monElmt>
                <monGen value="a"/>
              </monElmt>
              <one/>
            </monElmt>
          </label>
        </transition>
        <transition source="s6" target="s1">
          <label>
            <monElmt>
              <one/>
              <monElmt>
                <monGen value="u"/>
              </monElmt>
            </monElmt>
          </label>
        </transition>
        <initial state="s0"/>
        <initial state="s1"/>
        <final state="s1"/>
        <final state="s4"/>
      </transitions>
    </automatonStruct>
  </automaton>

</fsmxml>
