. ./defs || exit 1

set -e

VCSN=vcsn-int-b

$VCSN identity $int_b

# Make sure dot understands the output.
$VCSN dot-dump $int_b | dot

test `$VCSN eval $int_b "10#11"` = 1

test `$VCSN eval $int_b "10#12"` = 0

test `$VCSN eval $int_b "11"` = 1

$VCSN eval $int_b "1" 2> stderr && exit 1
grep 'Cannot parse' stderr
