
<automaton xmlns="http://vaucanson.lrde.epita.fr" name="u1">

  <labelType>
    <monoid generatorType="letters" type="product">
      <monoid generatorType="letters" type="free">
        <generator value="x"/>
        <generator value="y"/>
      </monoid>
      <monoid generatorType="letters" type="free">
        <generator value="u"/>
        <generator value="v"/>
      </monoid>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s2" in="y" src="s1"/>
      <transition dst="s1" out="u" src="s2"/>
      <transition dst="s0" out="v" src="s1"/>
      <transition dst="s1" in="x" src="s0"/>
      <initial state="s0"/>
      <initial state="s1"/>
      <final state="s1"/>
    </transitions>
  </content>

</automaton>
