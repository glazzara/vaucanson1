
<automaton xmlns="http://vaucanson.lrde.epita.fr" name="t1">

  <labelType>
    <monoid generatorType="letters" type="product">
      <monoid generatorType="letters" type="free">
        <generator value="a"/>
        <generator value="b"/>
      </monoid>
      <monoid generatorType="letters" type="free">
        <generator value="x"/>
        <generator value="y"/>
      </monoid>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s0" in="a" src="s1"/>
      <transition dst="s1" out="y" src="s0"/>
      <transition dst="s2" out="x" src="s1"/>
      <transition dst="s1" in="b" src="s2"/>
      <initial state="s1"/>
      <final state="s0"/>
      <final state="s1"/>
    </transitions>
  </content>

</automaton>
