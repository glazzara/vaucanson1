
<automaton xmlns="http://vaucanson.lrde.epita.fr" name="c1">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring operations="classical" set="Z"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
    </states>
    <transitions>
      <transition dst="s0" label="a+b" src="s0"/>
      <transition dst="s1" label="b" src="s0"/>
      <transition dst="s1" label="(2 a)+(2 b)" src="s1"/>
      <initial label="1" state="s0"/>
      <final label="1" state="s1"/>
    </transitions>
  </content>

</automaton>
