
<automaton xmlns="http://vaucanson.lrde.epita.fr" name="ladybird 6">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
      <generator value="c"/>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
      <state name="s3"/>
      <state name="s4"/>
      <state name="s5"/>
    </states>
    <transitions>
      <transition dst="s5" label="a" src="s4"/>
      <transition dst="s0" label="c" src="s4"/>
      <transition dst="s4" label="c" src="s4"/>
      <transition dst="s0" label="c" src="s3"/>
      <transition dst="s3" label="c" src="s3"/>
      <transition dst="s3" label="a" src="s2"/>
      <transition dst="s0" label="c" src="s1"/>
      <transition dst="s1" label="c" src="s1"/>
      <transition dst="s2" label="b" src="s2"/>
      <transition dst="s1" label="b" src="s1"/>
      <transition dst="s1" label="a" src="s0"/>
      <transition dst="s4" label="a" src="s3"/>
      <transition dst="s0" label="a" src="s5"/>
      <transition dst="s5" label="b" src="s5"/>
      <transition dst="s4" label="b" src="s4"/>
      <transition dst="s2" label="a" src="s1"/>
      <transition dst="s3" label="b" src="s3"/>
      <transition dst="s0" label="c" src="s2"/>
      <transition dst="s2" label="c" src="s2"/>
      <transition dst="s0" label="c" src="s5"/>
      <transition dst="s5" label="c" src="s5"/>
      <initial state="s0"/>
      <final state="s0"/>
    </transitions>
  </content>

</automaton>
