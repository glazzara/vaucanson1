<automaton name="b1" xmlns="http://vaucanson.lrde.epita.fr">
  <labelType>
    <monoid generators="letters" type="free">
      <generator value="a"/>
      <generator value="b"/>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>
  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
    </states>
    <transitions>
      <transition src="s0" dst="s0" label="a"/>
      <transition src="s0" dst="s0" label="b"/>
      <transition src="s0" dst="s1" label="b"/>
      <transition src="s1" dst="s1" label="a"/>
      <transition src="s1" dst="s1" label="b"/>
      <initial state="s0"/>
      <final state="s1"/>
    </transitions>
  </content>
</automaton>
