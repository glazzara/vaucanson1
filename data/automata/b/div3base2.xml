
<automaton xmlns="http://vaucanson.lrde.epita.fr" name="div3base2">

  <labelType>
    <monoid generatorType="letters" type="free">
      <generator value="0"/>
      <generator value="1"/>
    </monoid>
    <semiring operations="classical" set="B"/>
  </labelType>

  <content>
    <states>
      <state name="s0"/>
      <state name="s1"/>
      <state name="s2"/>
    </states>
    <transitions>
      <transition dst="s2" label="\0" src="s1"/>
      <transition dst="s1" label="\1" src="s0"/>
      <transition dst="s0" label="\0" src="s0"/>
      <transition dst="s1" label="\0" src="s2"/>
      <transition dst="s0" label="\1" src="s1"/>
      <transition dst="s2" label="\1" src="s2"/>
      <initial state="s0"/>
      <final state="s0"/>
    </transitions>
  </content>

</automaton>
